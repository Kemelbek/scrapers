window.onload = function() {
   
		
			var sec = 7;	 
			var interval;
			console.log('laod the iframe');
			taimer_init(sec);
		
		
	  
 } 

function iframeLoaded(callback) {
		callback(1);
}

 

function taimer_init(sec){
	
	$("#taimer_sec").append('00');
	if(isNaN(sec)) sec = 7;
	taimer_set(sec);
	interval = setInterval(taimer, 1000);
	var timerOnce = window.setTimeout("DivOff_popupsu_notds();", 8000);
}
function taimer_set(sec){
	var sec = sec;
	$("#taimer_sec").text(sec);
}
function taimer(){
	var sec = parseInt($("#taimer_sec").text());
	if(sec>0){
		
		console.log(sec);
		taimer_set(sec-1);
	}
	else{
		clearInterval(interval);
		DivOff_popupsu_notds();
	}
}
 
//Функция показа плавающего окна
function Show_popupsu_notds(){
	CookieTest=navigator.cookieEnabled; // определяем, включены ли cookies       
	if(CookieTest){     // если включены        
		cookie = GetCookie_popupsu('modal_banner'); // проверяем наличие cookies  cookie == null
		if (cookie == null){     // если cookies  не установлены, продолжаем
			$.post(banner_url+"action=banner_stat",	{bid: 593}, function(xml) {
				var answer = $("message", xml);
				/*if(answer.attr("result") == 1){
					console.log("+");
				} else console.log("-");*/
			});	
			print_modal();
		}
	  //print_modal();
	}    
}

function print_modal(){
	var ver = navigator.appVersion; // определяем браузер
	if (ver.indexOf('MSIE') != -1) 
	{     // если браузер - IE, прописываем код самого плавающего окна для IE
	document.write("<div id='PopWin_popupsu_notds' onclick='DivOff_popupsu_notds();PopShow_popupsu_notds();'  style='background: #000;width: 100%;height: 100%;cursor: pouner;z-index:9999999999;border:1px solid #666;position:absolute;"+ gor +";"+ vertic +"'>" 
	+" <table width='100%' height='80' cellspacing='0' cellpadding='0' style='background-color: "+ bgcolor +";'>" 
	+" <tr height='20'><td bgcolor='"+ bgcolor +"' style='padding-left: 10px; background-color: "+ bgcolor +"; text-align:left;border-bottom:solid 1px #000000'><b>" 
	+" <a target='_blank' class='fcl' style='font-size: 12px; color: "+ hcolor +"; " 
	+" font-family: Arial; text-decoration:none;' href='"+ url +"'>" 
	+" </a></td>" 
	+" <td id='modal_reklama_sec' bgcolor='"+ bgcolor +"' style='font-size:20px;font-weight:bold;text-align:center; padding-right: 3px; background-color: "+ bgcolor +";border-bottom:solid 1px #000000' align='right'>" 
	+" " 
	+" </td></tr>" 
	+" <tr><td height='60' width='100%' height='100%' colspan='2' bgcolor='"+ bgcolor +"'>" 
	+" <center><div style='display:inline-block;position:relative'><div><div id='' style='border:0px solid black;width:760px;height:550px;'><!--noindex--><div id='shveya_kyr' style='width:760px;height:550px;'><a  href='"+ url +"' target='_blank'><img src='/commercial/images/sinfo/"+imagename+"' border='0px' alt='реклама'></a><!--/noindex--></div><b style='position: absolute; top:-2px;right:-95px;'><a style='font-size: 19px; padding: 5px; color: black; font-family: Arial; text-decoration:none;' onclick='DivOff_popupsu_notds();PopShow_popupsu_notds();' title='Жабуу' target='_top' href='javascript://'>X</a></b></div>" 
	+"</center></tr>" 
	+" </tr></table></div>"); 
	if (document.all['PopWin_popupsu_notds'].style)
	window.setInterval("MovePop_popupsu_notds()", 10);
	window.onerror=null; 
	document.body.scrollTop=10; 
	}  
	else  
	{     // если браузер - не IE, прописываем код самого плавающего окна для остальных браузеров
	document.write("<div id='PopWin_popupsu_notds' onclick='DivOff_popupsu_notds();PopShow_popupsu_notds();' style='width:100%; height:100%; position:fixed;z-index:99999999; "+ gor +"; "+ vertic +";'>"); 
	document.write(" <table width='100%' height='100%' cellspacing='0' cellpadding='0' style='background-color: "+ bgcolor +"; z-index:1002; '>" 
	+" <tr height='20'><td align='left' style='padding-left: 10px;'><b>" 
	+" <a target='_blank' style='font-size: 12px; color: "+ hcolor +"; " 
	+" font-family: Arial; text-decoration:none;' class='fcl' href='"+ url +"'>" 
	+" </a></td>" 
	+" <td id='modal_reklama_sec' bgcolor='"+ bgcolor +"' style='font-size:20px;font-weight:bold;text-align:center; padding-right: 3px; padding-top:10px; background-color: "+ bgcolor +";' align='right'>" 
	+"" 
	+"</td></tr>"
	+" <tr><td height='100%' width='100%' colspan='2' bgcolor='"+ bgcolor +"' style='vertical-align:middle;text-align:center;'>"  
