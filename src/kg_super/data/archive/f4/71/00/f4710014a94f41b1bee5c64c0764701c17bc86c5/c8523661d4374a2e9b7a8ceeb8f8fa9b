<script type="text/javascript" >

	jQuery(function(f){

	$(".stdate").datepicker({ dateFormat: 'yy-mm-dd' });

	if($("body").find(".w-head").length !== 0 && $("body").find(".w-1").length !== 0 ){
		$("body").find(".news_text_big").removeClass("news_text_big");
	}else{
		//console.log("notfounded");
	}


	var cid = $("h1").attr("data-cid");
	//console.log(cid);
	$("li[data-cid='"+cid+"']").addClass("kactv");


    var element = f('#blfix');
    f(window).scroll(function(){
        element['fade'+ (f(this).scrollTop() > 500 ? 'In': 'Out')](500);
    });

	var h = $('.story_block').height();
	var h2 = $('.stor').height();
	$('.story_block').css({"height":"160px","overflow":"hidden"});
	$('.stor').css({"height":"160px","overflow":"hidden"});

	$(".vniz-buttonin").toggle(function(){
		$(".story_block").animate({height:''+h+''});
		$(".stor").animate({height:''+h2+''});
		$(this).css({"transform":"rotate(180deg)"});

	},function(){
		$(".story_block").animate({height:'160px'});
		$(".stor").animate({height:'160px'});
		$(this).css({"transform":"inherit"});});



	setInterval(function() {
	$(".vniz-buttonin").addClass("animated flash");

	}, 2000);

	setInterval(function() {
	$(".vniz-buttonin").removeClass("animated flash");
	},2500);

	$("#total-tektesh span").html("&nbsp;Тектеш кабарлар (<span>"+$(".total-tektesh").attr("data-tektesh")+"</span>)&nbsp;");

	$(".news_text_big").addtocopy({htmlcopytxt: '<br/><a href="'+window.location.href+'">'+window.location.href+'</a>', minlen:10, addcopyfirst: false});


	$(".close_dostup").click(function(){
		alert("Сиздин укугуңуз жок!");
	});
	
	

	$(".news_text_big img").click(function(){
		var nomodal = $(this).attr("data-noclick");
		
		if(nomodal ==1){
			
			
			
		}else{
			
			$(".modal").show();
			$("#img01").attr("src",$(this).attr("src"));
			$("#caption").text($("figcaption").text());
			
		}
		

	});

	$(".close").click(function(){
		$(".modal").hide();
	});

	$(".bytime").click(function(){

		$(".sh-bytime").toggle();
	});
        var weatherCities = $('.w-head');

        weatherCities.click(function(){
			var city = $(this).attr("data-city");
			$(".w-1[data-city!='"+city+"']").slideUp();
			$(".w-1[data-city ='"+city+"']").slideDown();

			$(".w-head[data-city!='"+city+"']").find(".w-down").html('<i class="fa fa-chevron-right" aria-hidden="true"></i>');

			$(".w-head[data-city ='"+city+"']").find(".w-down").html('<i class="fa fa-chevron-down" aria-hidden="true"></i>');

	});

	$('.ivideo').contents().on("click",".video-js",function(){
		//console.log("r");
		var id = $(this).attr("data-id");
		//console.log(id);
		var i =0;
	    var vid = $("iframe").filter(".ivideo:not([data-id ='"+id+"'])");

		vid.each(function(){
			var src = $(this).attr("src");
			$(this).attr("src",src);
			//console.log(src);

		});




	});

	$(".news_text_big").on("click",".muted-ver",function(){
		$(this).hide();
	});

	$(".one-n-img").on("click",".muted-ver",function(){
		$(this).hide();
	});

	$(".main-img").on("click",".muted-ver",function(){
		$(this).hide();
	});
    $('.story-link').on('click', function () {

        var $this = $(this);
        var urls = "/ajax_handler-mod/kabar/act/";
        $.ajax({
            type: 'POST',
            url: urls+"story_view",
            data: {sid:$this.data('id')},
            success: function(html) {

            },
            async:false
        });
    });
	var j =0;
	$(".news_text_big img").each(function(){
		var urls = "/ajax_handler-mod/kabar/act/";
		var img = $(this).attr("src");

		$(this).css({"padding":" 0 5px 0 0"});
		$(this).attr("data-id",j);
		img = img.split("/");

		$.ajax({
			type: 'POST',
			url: urls+"imgMute",
			data: {img:img[5]},
			success: function(html) {
				 var answer = $("div", html);
				 if(answer.html() == 1){
					 var style = $(".news_text_big img[data-id='"+j+"']").attr("style");
					 var w = $(".news_text_big img[data-id='"+j+"']").attr("width");
					 var h = $(".news_text_big img[data-id='"+j+"']").attr("height");
					 var current = $(".news_text_big img[data-id='"+j+"']");
					 current.removeAttr("style");
					 current.removeAttr("width");
					 current.removeAttr("height");
					 current.css({"width":"100%"});

					 $(".news_text_big img[data-id='"+j+"']").before('<div id="mute'+j+'" style="'+style+'width:'+w+'px; height:'+h+'px; position:relative; margin:0 5px 5px 0;"></div>');
					 $('#mute'+j+'').append(current);
					 $('#mute'+j+'').append("<div class='muted-ver'><div class='m1'><p style='font-size:40px !important'><i class='fa fa-eye-slash' aria-hidden='true'></i></p><p style='font-size: 12px!important;    font-style: normal;'>Эскертүү! Сезгенчээк адамдарга бул сүрөттү кароо сунушталбайт.</p></div></div>");


				 }

				},
			async:false
	   });


		j++;
	});
        });




</script>
