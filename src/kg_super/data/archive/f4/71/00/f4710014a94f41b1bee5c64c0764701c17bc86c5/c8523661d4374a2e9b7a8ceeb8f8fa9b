<script>
	$(function(){
		var temp = [];
		var i = 0;
		$(".oth-n-thum").each(function(){ 
			temp[i] = $(this).height();
		i++; 	
		});
		temp.sort(function(a,b){return b - a}); 
		$(".oth-n-thum").css({"height":""+temp[0]+"px"});
		$(".other_news_line").css({"height":""+temp[0]+"px"});
		
		
		$(".right-oth-b").click(function(){
			$(".left-oth-b").show();
			var left = parseInt($(".othr-n-item").css('left'));	
			var width = parseInt($(".othr-n-item").css('width'));
			 		
			if(left<0){
				left = left * -1;				
			}
			left = left +760;
			
			if(left<width){
				$(".othr-n-item").animate({left: "-"+left+"px"});
				left = left +760+5;
			
				if(left>width){
				$(this).hide();
				}
				
			}else{
				$(this).hide();
			}
		
		});
		
		$(".left-oth-b").click(function(){
			$(".right-oth-b").show();
			var left = parseInt($(".othr-n-item").css('left'));	
			var width = parseInt($(".othr-n-item").css('width'));
			
			if(left<0){
				left = left * -1;
				left = left -760;
				$(".othr-n-item").animate({left: "-"+left+"px"});
				left = left -760;
				left = left * -1;
				//console.log(width);
			    //console.log(left);	
				if(left>0){
					$(this).hide();
				}
				
				
			}else{
				$(this).hide();
				
			}
		});
	
	 
		
	 var total = $(".news_text_big p").length;	 
	
	  $(".oth-n-thum").hover(function(){
		  $(this).toggleClass("animated pulse");
	  });
		
	});
</script>		</br>


			<div style ="width:614px ; height:75px; background: url(/images/janylyk/new_22.JPG) no-repeat;">
			<div style="padding: 8px 0 0 10px; font-weight: bold; "> Урматтуу колдонуучу! Эгер сизде коомчулуктун көңүлүн буруп, талкууну талап кылган жаңылыгыӊыз болсо биз менен бөлүшүӊүз.</div>
			<a><div style="width: 500px; height:30px; float: left; margin: 7px 0 0 5px;"> </div></a>
		</div>
