window.onload = function() {
   
		
			var sec = 7;	 
			var interval;
			console.log('laod the iframe');
			taimer_init(sec);
		
		
	  
 } 

function iframeLoaded(callback) {
		callback(1);
}

 

function taimer_init(sec){
	
	$("#taimer_sec").append('00');
	if(isNaN(sec)) sec = 7;
	taimer_set(sec);
	interval = setInterval(taimer, 1000);
	var timerOnce = window.setTimeout("DivOff_popupsu_notds();", 8000);
}
function taimer_set(sec){
	var sec = sec;
	$("#taimer_sec").text(sec);
}
function taimer(){
	var sec = parseInt($("#taimer_sec").text());
	if(sec>0){
		
		console.log(sec);
		taimer_set(sec-1);
	}
	else{
		clearInterval(interval);
		DivOff_popupsu_notds();
	}
}
 
//Функция показа плавающего окна
function Show_popupsu_notds(){
	CookieTest=navigator.cookieEnabled; // определяем, включены ли cookies       
	if(CookieTest){     // если включены        
		cookie = GetCookie_popupsu('modal_banner'); // проверяем наличие cookies  cookie == null
		if (cookie == null){     // если cookies  не установлены, продолжаем
			$.post(banner_url+"action=banner_stat",	{bid: 593}, function(xml) {
				var answer = $("message", xml);
				/*if(answer.attr("result") == 1){
					console.log("+");
				} else console.log("-");*/
			});	
			print_modal();
		}
	  //print_modal();
	}    
}

