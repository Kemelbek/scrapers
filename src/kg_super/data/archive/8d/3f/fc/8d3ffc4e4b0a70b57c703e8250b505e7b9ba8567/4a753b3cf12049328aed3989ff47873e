	<script>
	$(function(){   
		 
		var $win = $(".body2"); // or $box parent container
		var $box = $(".super");
		var $input = $("input[type='submit']");
		var $sort = $(".sort");
		var $button = $("button");
		var $cloase = $(".ui-datepicker-calendar");  
		var $calenday = $(".ui-datepicker-calendar > td");  
		var $calendaytda = $(".ui-state-default");  
		
		$win.on("click.Bst", function(event){	
			 
			console.log(event.pageY);  
			 if ( 
				$box.has(event.target).length == 0 //checks if descendants of $box was clicked
				&&
				!$box.is(event.target) //checks if the $box itself was clicked
				&&
				!$input.is(event.target)
				&&
				!$sort.is(event.target)  
				&&
				!$button.is(event.target)
				&&
				!$calenday.is(event.target)  
				&&
				!$cloase.is(event.target)
				&&
				!$calendaytda.is(event.target)
				   
			  ){				  
				  let url = $(".brandstat a").attr("href");
				  
				   if(event.pageX >=1000 &&  event.pageX <=1206 && event.pageY>=2200 && event.pageY <=2600){
					   
				   }else{
					      var array = [];
						  var tot=0;
						  $(".brandstat a").each(function(){
							  var url = $(this).attr("href");
							  if(url!=''){
								  tot++;
								  array.push(url);
							  }
						  });
						  
						  var n = Math.floor(Math.random() * tot + 1);
						  console.log(tot);
						  
										  
						  window.open(array[n-1]);  
				   }
				  				  
				  //
							
				} else {
							//console.log("you clicked inside the box");
				} 
		});
		
		
		
	});
</script>

