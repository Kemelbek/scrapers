		function my_getbyid(id) {
			ids = null;
			if (document.getElementById) { ids = document.getElementById(id); } 
			else if (document.all) { ids = document.all[id]; }
			else if (document.layers) { ids = document.layers[id]; }
			return ids;
		}
		
		
		
