function show_modal(){
	document.getElementById('PopWin_popupsu_notds').style.display='block'; 
}
//document.write("<a href='javascript:show_modal();' onclick=''>Показать</a>");
Show_popupsu_notds(); // вызываем функцию показа плавающего окна

// Функция всплывающего окна 
function PopShow_popupsu_notds(url){
	if(top.self==window){ // проверка на фрейм
		CookieTest=navigator.cookieEnabled; // проверка, включены ли cookies        
		if(CookieTest){             // если cookies включены
			cookie = GetCookie_popupsu('modal_banner'); // проверяем наличие cookie
			if (cookie == null) {            // если cookies нет,
				/*var ExpDate = new Date (); 
				ExpDate.setTime(ExpDate.getTime() + (hour * 60 * 60 * 1000)); // присваиваем переменной ExpDate (тип даты) время, равное настоящему + количество часов, указанное в переменной hours,*/
				var cookieDate = new Date ();
				if(cookieDate.getHours()<12){
					cookieDate.setHours(11);
					cookieDate.setMinutes(59);
					cookieDate.setSeconds(59);
				}
				else{
					cookieDate.setHours(23);
					cookieDate.setMinutes(59);   
					cookieDate.setSeconds(59);
				}
				SetCookie_popupsu('modal_banner','1',cookieDate, "/"); // записываем cookies значением 1 и временем удаления ExpDate,
				DivOff_popupsu_notds(); // убираем плавающее окно,
				this.window.focus(); // возвращаем фокус на окно с этим скриптом
			} 
		}
	}  
}
 
//Дальше НИЧЕГО не меняйте!!!
function getCookieVal_popupsu(offset) {
	var endstr = document.cookie.indexOf (";", offset);
	if (endstr == -1)
		endstr = document.cookie.length;
	return unescape(document.cookie.substring(offset, endstr));
}
 
function GetCookie_popupsu (name) {  
	var arg = name + "=";  
	var alen = arg.length;  
	var clen = document.cookie.length;  
	var i = 0;  
	while (i < clen){  
		var j = i + alen;  
		if (document.cookie.substring(i, j) == arg)  
			return getCookieVal_popupsu(j);  
		i = document.cookie.indexOf(" ", i) + 1;  
		if (i == 0) break;  
	}  
	return null; 
} 
function SetCookie_popupsu (name, value) {  
	var argv = SetCookie_popupsu.arguments;  
	var argc = SetCookie_popupsu.arguments.length;  
	var expires = (argc > 2) ? argv[2] : null;  
	var path = (argc > 3) ? argv[3] : null;  
	var domain = (argc > 4) ? argv[4] : null;  
	var secure = (argc > 5) ? argv[5] : false;  
	document.cookie = name + "=" + escape (value) +  
	((expires == null) ? "" : ("; expires=" + expires.toGMTString())) +  
	((path == null) ? "" : ("; path=" + path)) +  
	((domain == null) ? "" : ("; domain=" + domain)) +  
	((secure == true) ? "; secure" : "");
}
 
//Функция скрытия плавающего окна
function DivOff_popupsu_notds() 
{   
	pelement = document.getElementById('PopWin_popupsu_notds');
	if(pelement){
		pelement.style.display='none';
		PopShow_popupsu_notds();
	}
}
 
//Функция движения плавающего окна вместе со страницей в IE
function MovePop_popupsu_notds() {
	if (verticie>-1){
		if (document.body.scrollTop==0)
			document.all['PopWin_popupsu_notds'].style.top=document.documentElement.scrollTop+verticie; 
		else
			document.all['PopWin_popupsu_notds'].style.top=document.body.scrollTop+verticie;
	}
	else{
		document.all['PopWin_popupsu_notds'].style.top=document.body.scrollTop+document.body.clientHeight-100;
	}
}

function getBanStat(linkto){
	var XHR = ("onload" in new XMLHttpRequest()) ? XMLHttpRequest : XDomainRequest;
	var xhr = new XHR();
	xhr.open('GET', '//stats.super.kg/banner/' + linkto, true);
	xhr.send();
}
