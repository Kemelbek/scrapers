<script>
	$(function(){
		$(".nn-th-left").hide();
		var classcount = $('.nn-thumb').length;
		if(classcount<=7){ 
			$(".nn-th-left").hide();
			$(".nn-th-right").hide();
		}
		
		var realhth = $(".main-img img").height();
		if(realhth != 0){
			console.log("inside");
			$(".nn-gallery").css({"height":realhth+"px"});
		}else{
			setTimeout(function(){ 
			var rht = $(".main-img img").height();
			$(".nn-gallery").css({"height":rht+"px"});
			realhth = rht;  
			console.log(rht);
			
			}, 2000); 
			
			console.log("out");
		} 
		  
		 
				
		$(".nn-thumb").hover(function(){
			$(this).toggleClass("img-hover");
		});
		
		$(".nn-thumb").click(function(){
			$(".nn-thumb").removeClass("img-active");
			$(".main-img img").removeAttr("style"); 
			$(this).addClass("img-active");
			
			var mute = $(this).attr("data-mute"); 
			
			var decrpt = $(this ).find(".desc-thum").text();
			if(decrpt!='' && typeof(decrpt) != "undefined" ){
				var hscls = $(".main-img").find('.gg-descrr');
				if(hscls.length > 0){
					$(".gg-descrr").text(decrpt);
					$(".gg-descrr").show();  
				}else{
					$(".main-img").append("<div class='gg-descrr'>"+decrpt+"</div>"); 
					$(".gg-descrr").show();
				}     
				 
			}else{
				$(".gg-descrr").hide(); 
			}
			
			
			var id = $(this).attr("data-id");
			$(".main-img").attr("data-id",id);
			//$(".nn-line-2").css({"left":"-"+left+"px"});
			var link = $(this).children().attr("src");
			if($(this).attr("data-type") == 'vertical'){
					$(".main-img").removeClass("g-img");
					$(".main-img").addClass("v-img");
			}else{
					$(".main-img").removeClass("v-img");
					$(".main-img").addClass("g-img");
			}
			if($(this).hasClass("thum-v"))
			{	
				$(".main-img").removeClass("g-img");
				$(".main-img").addClass("v-img");
				$(".v-img img").css({"height":realhth+"px"});
			}
		
			$(".main-img img").attr("src",link);
			
			var checkwidth = $(".main-img img").height();
			console.log(checkwidth);
			if(checkwidth>realhth){
				$(".main-img img").css({"width":"auto","height":realhth+"px"});
			} 
			if(mute ==1){
				$(".muted-ver[data-gall='1']").show();
			}else{
				$(".muted-ver[data-gall='1']").hide();
			}
			
			
		});
		
		$(".nn-th-right").click(function(){
						
			var left = 7 * 107;
			var rleft = parseInt($(".nn-line-2").css("left"));
			if(rleft<0){
				rleft = rleft * -1;
			}
			left = rleft + left;
			console.log(left);
			
			var width = $(".nn-line-2").width();
			width = width - left;
			console.log(width);
			if(width<750){
				$(this).hide();   
			}
			$(".nn-th-left").show();
			$(".nn-line-2").animate({left: '-'+left+'px'});
		
		});   
		
		$(".nn-th-left").click(function(){
			$(".nn-th-right").show();
			var left = 7 * 107; 
			var rleft = parseInt($(".nn-line-2").css("left"));  
			if(rleft<0){
				rleft = rleft * -1;
			}
			left = rleft - left;
			if(left < 7 * 107){
				 $(this).hide();
			}
			
			$(".nn-line-2").animate({left: '-'+left+'px'});
		});
		
		$(".nn-gallery").hover(function(){
			var id = $(".main-img").attr("data-id");
			
			
			if(id == 0){
					$(".mn-right").show(); 
			}else if(id == classcount-1){
					$(".mn-left").show(); 
			}else{
				$(".mn-right").show(); 
				$(".mn-left").show(); 
			}
			
		 
			  
		},function(){
			$(".mn-left").hide();
			$(".mn-right").hide();   
			
		});        
		
		$(".mn-right").click(function(){
			$(".mn-left").show();
			$(".main-img img").removeAttr("style"); 
			var id = $(".main-img").attr("data-id");
			id++;
			if(id != classcount ){
			var mute = $(".nn-thumb[data-id='"+id+"']").attr("data-mute"); 
			if($(".nn-thumb[data-id='"+id+"']").hasClass("thum-v"))
			{	
				$(".main-img").removeClass("g-img");
				$(".main-img").addClass("v-img");
				$(".v-img img").css({"height":realhth+"px"});
				console.log("kana"); 
			}else{
				$(".main-img").removeClass("v-img");
				$(".main-img").addClass("g-img");
			}
			
			var src = $(".nn-thumb[data-id='"+id+"'] img").attr("src");
			var descr = $(".nn-thumb[data-id='"+id+"']").find(".desc-thum").text();
			
			if(descr!='' && typeof(descr) != "undefined" ){
				var hscls = $(".main-img").find('.gg-descrr');
				if(hscls.length > 0){
					$(".gg-descrr").text(descr);
					$(".gg-descrr").show();
				}else{
					$(".main-img").append("<div class='gg-descrr'>"+descr+"</div>"); 
					$(".gg-descrr").show();
				}   
				
				
			}else{
				$(".gg-descrr").hide();  
			}
			
			
			$(".main-img").attr("data-id",id);
			$(this).attr("data-count",id);
			
		   
			    
				$(".main-img img").attr("src",src);
			   
			
			var checkwidth = $(".main-img img").height();
			if(checkwidth>realhth){
				$(".main-img img").css({"width":"auto","height":realhth+"px"});
			}  
				if(mute ==1){
				$(".muted-ver[data-gall='1']").show();
				}else{
				$(".muted-ver[data-gall='1']").hide();
				}
			}
			

			
			if(id ==classcount-1){
				$(this).hide();   
			}
			
		});  
		  
		$(".mn-left").click(function(){
			
			$(".mn-right").show();
			$(".main-img img").removeAttr("style"); 
			var id = $(".main-img").attr("data-id");
			id = id - 1; 
			var mute = $(".nn-thumb[data-id='"+id+"']").attr("data-mute");
			
			
			
			
			
			if($(".nn-thumb[data-id='"+id+"']").hasClass("thum-v"))
			{	
				$(".main-img").removeClass("g-img");
				$(".main-img").addClass("v-img");
				$(".v-img img").css({"height":realhth+"px"});
			}else{
				$(".main-img").removeClass("v-img");
				$(".main-img").addClass("g-img");
			}  
			
			var descr = $(".nn-thumb[data-id='"+id+"']").find(".desc-thum").text();
			
			if(descr!='' && typeof(descr) != "undefined" ){
				var hscls = $(".main-img").find('.gg-descrr');
				if(hscls.length > 0){
					$(".gg-descrr").text(descr);
					$(".gg-descrr").show();
				}else{
					$(".main-img").append("<div class='gg-descrr'>"+descr+"</div>"); 
					$(".gg-descrr").show();
				}
				
			}else{
				$(".gg-descrr").hide();  
			} 		
			
			var src = $(".nn-thumb[data-id='"+id+"'] img").attr("src");
			$(".main-img").attr("data-id",id);
			$(this).attr("data-count",id);
			$(".main-img img").attr("src",src);
			var checkwidth = $(".main-img img").height();
			if(checkwidth>realhth){
				$(".main-img img").css({"width":"auto","height":realhth+"px"});
			} 
			if(id ==0){
				$(this).hide();   
			}
			
			if(mute ==1){
				$(".muted-ver[data-gall='1']").show();
			}else{
				$(".muted-ver[data-gall='1']").hide();
			}
		
		}); 
		
		
	});

</script>	<div class="newscat_43 news_text_big" style="width:720px;">
			 
			
			<p>Бүгүн, 31-августта ырчы <strong>Элмира Кайып кызы</strong> 42 жашында дүйнө салды. Бул тууралуу ырчынын сахналаштары билдиришти.</p>
<p>Элмира Кайып кызынын туугандарынын айтымында, ырчынын бооруна операция жасалып, ал эсине келбей каза болгон.</p>
<p>Ырчы Элмира Кайып кызы 1975-жылы 4-январь күнү Ысык-Көл облусуна караштуу Жети-Өгүз районундагы Ак-Терек айылында туулган. Т.Сатылганов атындагы эстрадалык студияны жана Б.Бейшеналиева атындагы искусство институтун аяктаган.</p>
<p>Алгач Р.Абдыкадыровдун "Ак куулар сени сагынар" деген ыры менен К.Орозов атындагы оркестрдин коштоосунда сахнага чыккан. Эстрадада Шамшыбек Өтөбаевдин "Калды эсимде" деген ыры менен таанылган. Элмира Кайып кызынын аткаруусундагы <a href="http://www.super.kg/media/audio/47251">"Булбулум"</a> аттуу чыгарма элге кеңири тараган. Ырчы акыркы мезгилдерде Мамлекеттик чек ара кызматынын ыр жана бий ансамблинин насаатчысы болуп иштеп келген.</p>
<p><strong>SUPER.KG</strong> порталынын эмгек жамааты ырчынын көз жумганына байланыштуу үй-бүлөсүнүн, жакындарынын аза кайгысын тең бөлүшүп, терең кайгыруу менен көңүл айтат.</p>
<p> </p>			
						
												
			
			
