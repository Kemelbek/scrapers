	+"</center></tr>" 
	+" </tr></table></div>"); 
	if (document.all['PopWin_popupsu_notds'].style)
	window.setInterval("MovePop_popupsu_notds()", 10);
	window.onerror=null; 
	document.body.scrollTop=10; 
	}  
	else  
	{     // если браузер - не IE, прописываем код самого плавающего окна для остальных браузеров
	document.write("<div id='PopWin_popupsu_notds' onclick='DivOff_popupsu_notds();PopShow_popupsu_notds();' style='width:100%; height:100%; position:fixed;z-index:99999999; "+ gor +"; "+ vertic +";'>"); 
	document.write(" <table width='100%' height='100%' cellspacing='0' cellpadding='0' style='background-color: "+ bgcolor +"; z-index:1002; '>" 
	+" <tr height='20'><td align='left' style='padding-left: 10px;'><b>" 
	+" <a target='_blank' style='font-size: 12px; color: "+ hcolor +"; " 
	+" font-family: Arial; text-decoration:none;' class='fcl' href='"+ url +"'>" 
	+" </a></td>" 
	+" <td id='modal_reklama_sec' bgcolor='"+ bgcolor +"' style='font-size:20px;font-weight:bold;text-align:center; padding-right: 3px; padding-top:10px; background-color: "+ bgcolor +";' align='right'>" 
	+"" 
	+"</td></tr>"
	+" <tr><td height='100%' width='100%' colspan='2' bgcolor='"+ bgcolor +"' style='vertical-align:middle;text-align:center;'>"  
