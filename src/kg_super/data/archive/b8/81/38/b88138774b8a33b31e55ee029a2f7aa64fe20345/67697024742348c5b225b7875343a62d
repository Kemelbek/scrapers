	if (ver.indexOf('MSIE') != -1) 
	{     // если браузер - IE, прописываем код самого плавающего окна для IE
	document.write("<div id='PopWin_popupsu_notds' onclick='DivOff_popupsu_notds();PopShow_popupsu_notds();'  style='background: #000;width: 100%;height: 100%;cursor: pouner;z-index:9999999999;border:1px solid #666;position:absolute;"+ gor +";"+ vertic +"'>" 
