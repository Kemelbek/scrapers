<script type="text/javascript">
function search_filter(a,b){
	my_getbyid('menulink').innerHTML=a;
	my_getbyid('where_to_search').value=b;
	my_getbyid('menu')
}
function onch(){
	a = document.getElementById('where_to_search').value;
	if (a=='forum') {
	    document.getElementById('search_f').action ='/superstan/index.php?app=core&module=search&do=search&fromMainBar=1';
	    document.getElementById('search_f').method ='POST';
	}
    else{
        document.getElementById('search_f').action ='/search/';
        document.getElementById('search_f').method ='GET';
    }
}
</script>
